{
    "env": {
        "development": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "corejs": "3.46",
                        "useBuiltIns": "entry"
                    }
                ],
                "@babel/preset-typescript",
                "@beanbag/ink/babel-preset"
            ]
        }
    },
    "plugins": [
        "@babel/plugin-external-helpers",
        [
            "@babel/plugin-proposal-decorators",
            {
                "version": "legacy"
            }
        ],
        "babel-plugin-dedent",
        "babel-plugin-django-gettext"
    ],
    "sourceMaps": true,
    "targets": [
        "baseline widely available"
    ]
}
